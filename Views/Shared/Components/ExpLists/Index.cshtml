@model List<Portfolio.Model.ExpModel>


    @foreach (var m in Model)
    {
        <div class="row">
            <div class="col-1-of-2">
                <div class="row">
                    <div class="col-1-of-2">
                        <h3 class="heading-tertiary u-margin-bottom-m">{@m.Position}r</h3>
                        <p class="paragraph">
                            {@m.Company}
                        </p>
                    </div>
                    <div class="col-1-of-2">
                        <h3 class="heading-tertiary u-margin-bottom-m">FullTime</h3>
                        <p class="paragraph">
                           {@m.Year}
                        </p>
                    </div>
                </div>
            </div>
            <div class="col-1-of-2">
                <h3 class="heading-tertiary u-margin-bottom-m">Tech Stack</h3>
                <p class="paragraph">
                    {@m.Stack}
                </p>
            </div>

        </div>
        <div class="row">
            <hr />
        </div>
       
    }

